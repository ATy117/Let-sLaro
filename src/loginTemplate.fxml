      <ImageView fitHeight="153.0" fitWidth="363.0" layoutX="123.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@resources/Title.png" />
         </image>
      </ImageView>
      <Label layoutX="141.0" layoutY="225.0" text="Enter Username:" textFill="#c891f3">
         <font>
            <Font name="Cartoonist Kooky" size="24.0" />
         </font>
      </Label>
      <JFXTextField fx:id="usernameField" focusColor="#03768b" layoutX="199.0" layoutY="264.0" promptText="username" unFocusColor="#c891f3">
         <font>
            <Font name="Cartoonist Kooky" size="20.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="enterBtn" layoutX="274.0" layoutY="333.0" />
   </children>
</AnchorPane>
